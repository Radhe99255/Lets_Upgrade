Question - 17 - Solution : Reverse integer 16

import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        
        Scanner sc = new Scanner(System.in);
        int n,on;
        n = sc.nextInt();
        on = n;
        
        double INT32_MIN = Math.pow(-2,31);            // (-2**31 )  lower range of 32-bit integer
        double INT32_MAX = Math.pow(2,31) - 1;            // (2**31 - 1)  higher range of 32-bit integer
        
        int last,rem,rev = 0;
        
        while(n!=0)
        {
            rem = n%10;
            rev = rev*10 + rem;
            n = n/10;
        }
     
        
        if(rev < INT32_MIN || rev > INT32_MAX)
        {
            System.out.println(0);
        }
        
        System.out.println(rev);
    }
}
