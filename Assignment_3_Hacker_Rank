Question - 5 - Solution : Two Line Star Pattern

import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        
        Scanner sc = new Scanner(System.in);

        int i,j,n;
        n = sc.nextInt();
        
        for(i=1;i<=n;i++)
        {
            System.out.print("*");
            
            for(j=1;j<=n-2;j++)
            {
                System.out.print(" ");
            }
            
            System.out.print("*");
            System.out.println();
        }
    }
}
