Question - 6 - Solution : Leading spaces pyramid

import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        
        Scanner sc = new Scanner(System.in);

        int i,j,k,n;
        
        n = sc.nextInt();
        
        for(i=1;i<=n;i++)
        {
            for(j=1;j<=n-i;j++)
            {
                System.out.print(" ");
            }
            
            for(k=1;k<=i;k++)
            {
                System.out.print("*");
            }
            
            System.out.println();
        }
    }
}
