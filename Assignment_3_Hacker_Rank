Question - 24 - Solution : Star Pattern II

import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        
        Scanner sc = new Scanner(System.in);
        int n;
        n = sc.nextInt();
        
        int i,j,k;
        
        for(i=1;i<=n;i++)
        {
            System.out.print("*");
        }
        
        System.out.println();

        for(j=1;j<=n-2;j++)
        {
            System.out.print("*");
            for(k=1;k<=n-2-j;k++)
            {
                System.out.print(" ");
            }
            System.out.print("*");
            System.out.println();
        }
        System.out.print("*");
    }
}
