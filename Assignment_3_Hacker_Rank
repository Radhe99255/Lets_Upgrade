Question - 16 - Solution : Count the digits 2

import java.io.*;
import java.util.*;

public class Solution {
    
    public static int cndigit(int n)
    {
        int rem, count = 0;
        
        if(n==0||n==1)
        {
            count++;
            
            return count;
        }
            
        while(n>0)
        {
            rem = n%10;
            n = n/10;
            count++;
        }
        
        return count;
    }
    

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        
        Scanner sc = new Scanner(System.in);
        
        int n,t,i;
        t = sc.nextInt(); // take number of test cases
        
        for(i=1;i<=t;i++)
        {
            n = sc.nextInt(); // take number as input
            System.out.println(cndigit(n));
        }
        
    }
}
