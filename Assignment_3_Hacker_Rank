Question - 25 - Solution : Photo Frame Pattern

import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        
        Scanner sc = new Scanner(System.in);
        int n;
        n = sc.nextInt();
        
        int i,j;
        
        for(i=1;i<=n;i++)
        {
            for(j=1;j<=n;j++)
            {
                if(i==1 || i==n || j==1 || j==n) 
                {
                     System.out.print("*");
                } 
                
                else 
                {
                     System.out.print(" ");
                }
            }
            
             System.out.println();
        }
        
        /*
