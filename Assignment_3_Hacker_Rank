        n = sc.nextInt();
        
        int i1,i2,j,k;
        
        for(i1=1;i1<=n;i1++)
        {
            System.out.print("*");
        }
        
        System.out.println();
        
        for(j=1;j<=n-2;j++)
        {
            System.out.print("*");
            for(k=1;k<=n-2;k++)
            {
                System.out.print(" ");
            }
            System.out.print("*");
            System.out.println();
        }
        
        for(i2=1;i2<=n;i2++)
        {
            System.out.print("*");
        }
        */
    }
}
